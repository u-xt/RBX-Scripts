                                                    while wait(0.08) and plr and p do
                                                        f.UpdateHeadUI(ht)
                                                    end
                                                end)
                                                --table.insert(headsupdatelist, ht)
                                                ht.Parent = p
                                            end
                                        end
                                        end)
                                    end
                                end
