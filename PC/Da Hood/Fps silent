                                    if p:IsA("BasePart") and p.Name ~= "HumanoidRootPart" then
                                        if charmsesp then
                                        local urmom = Instance.new("BoxHandleAdornment")
                                        urmom.ZIndex = 10
                                        urmom.AlwaysOnTop = true
