-- Scripts:

local function BCYKF_fake_script() -- MainHolder.LocalScript 
	local script = Instance.new('LocalScript', MainHolder)

	--pasted btw
	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.5
	local dragStart = nil
	local startPos = nil
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
	
end
coroutine.wrap(BCYKF_fake_script)()


local COLOURN = Color3.fromRGB -- bri ish moment init
local Colours = { -- bri'ish moment
	["red"] = COLOURN(250, 1, 1);
	["green"] = COLOURN(1, 250, 1);
	["blue"] = COLOURN(1, 1, 250);
	["yellow"] = COLOURN(237, 255, 38);
	["black"] = COLOURN(1, 1, 1);
	["white"] =COLOURN(250, 250, 250);
}

local function GetPlr(String)
	local TotalFound = 0
	local Found = {}
	local strl = String:lower()
	for i,v in pairs(game.Players:GetPlayers()) do
		if TotalFound == 0 and v.Name:lower():sub(1, #String) == String:lower() or v.DisplayName:lower():sub(1, #String) == String:lower() then
			table.insert(Found,v.Name)
			TotalFound = 1
		end
	end
	if #Found >0 then
		Found = Found[1]
	else
		Found = nil
	end 
	return Found
end
local CurrentTarg = nil
local AimView = false
local AimPov = false
local DesyncTog = false
local DesyncMod = 2
local PovMode = 1
local MousePos = false
local LookPos = false


--toggles
ViewAim.MouseButton1Click:Connect(function()
	if AimView == false then
		AimView = true
		ViewAim.TextColor3 = Colours["green"]
	else
		AimView = false
		ViewAim.TextColor3 = Colours["red"]
	end
end)
ViewPos.MouseButton1Click:Connect(function()
	if AimPov == false then
		AimPov = true
		ViewPos.TextColor3 = Colours["green"]
	else
		AimPov = false
		ViewPos.TextColor3 = Colours["red"]
	end
end)
TogDesync.MouseButton1Click:Connect(function()
	if DesyncTog == false then
		DesyncTog = true
		TogDesync.TextColor3 = Colours["green"]
	else
		DesyncTog = false
		TogDesync.TextColor3 = Colours["red"]
	end
end)

ViewPosMode.MouseButton1Click:Connect(function()
	if ViewPosMode.Text == "pov mode: aim point" then
		PovMode = 2
		ViewPosMode.Text = "pov mode: look direction"
	else
		PovMode = 1
		ViewPosMode.Text = "pov mode: aim point"
	end
end)

Desyncmode.MouseButton1Click:Connect(function()
	if Desyncmode.Text == "Desync mode:1" then
		DesyncMod = 2
		Desyncmode.Text = "Desync mode:2"
	else
		DesyncMod = 1
		Desyncmode.Text = "Desync mode:1"
	end
end)

AimPos.MouseButton1Click:Connect(function()
	if LookPos == false then
		LookPos = true
		AimPos.TextColor3 = Colours["green"]
	else
		LookPos = false
		AimPos.TextColor3 = Colours["red"]
	end
end)

LookDir.MouseButton1Click:Connect(function()
	if MousePos == false then
		MousePos = true
		LookDir.TextColor3 = Colours["green"]
	else
		MousePos = false
		LookDir.TextColor3 = Colours["red"]
	end
end)

InputTarg.FocusLost:Connect(function()
	local arg = InputTarg.Text
	local check = GetPlr(arg)
	if check ~= nil then
		CurrentTarg = check
		InputTarg.Text = check
	else
		CurrentTarg = nil
	end
end)


--desync beanbot moment :(
local MakePart = function(c)
	local hold = Instance.new("Part", workspace)
	local bbg =  ColorSequence.new({
		ColorSequenceKeypoint.new(0, c),
		ColorSequenceKeypoint.new(1, c),
	})
	local prt1 = Instance.new("Part", hold)
	prt1.Size = Vector3.new(0, 0, 0)
	prt1.Massless = true
	prt1.Transparency = 1
	prt1.CanCollide = false
	prt1.Anchored = true
	local Attat = Instance.new("Attachment", prt1)
	local prt2 = Instance.new("Part", hold)
	prt2.Size = Vector3.new(0, 0, 0)
	prt2.Transparency = 0
	prt2.CanCollide = false
	prt2.Anchored = true
	prt2.Material = Enum.Material.ForceField
	prt2.Color = c
	prt2.Massless = true
	local Attat2 = Instance.new("Attachment", prt2)
	local semem = Instance.new("Beam", prt1)
	semem.FaceCamera = true
	semem.Color = bbg
	semem.Attachment0 = Attat
	semem.Attachment1 = Attat2
	semem.LightEmission = 16
	semem.LightInfluence = 16
	semem.Width0 = 0.25
	semem.Width1 = 0.25
	semem.Texture = "1"
	semem.TextureSpeed = 5
	semem.TextureLength = 5

	return prt1,prt2
end
function HasGun()
	local plr = game:GetService("Players")[CurrentTarg]
	if plr.Character then
		for i, v in pairs(plr.Character:GetChildren()) do
			if v:FindFirstChild("Ammo") then
				return true
			end
		end
	end
	return false
end
local Look1,Look2 = MakePart(Color3.new(0.117647, 1, 0.101961))
local Mouse1,Mouse2 = MakePart(Color3.new(1, 0.0117647, 0.027451))

local possing = false
local runitback = function()
	if CurrentTarg ~= nil then
		local plr = game:GetService("Players")[CurrentTarg]
		local thrp = plr.Character.HumanoidRootPart
		local ms = plr.Character.BodyEffects.MousePos.Value
		local look = thrp.CFrame.lookVector*10
		local cam = game.Workspace.CurrentCamera
		if plr.Name ~= game:GetService("Players").LocalPlayer.Name then
			plr.Character.Humanoid.AutoRotate = false
		end
		if AimView == true then
			if MousePos == true and HasGun() then
				ms = Vector3.new(ms.X - 0.1, ms.Y + 0.4, ms.Z)
				Mouse1.Transparency = 0
				Mouse2.Transparency = 0
				Mouse1.Position = Vector3.new(thrp.Position.X, thrp.Position.Y+0.2, thrp.Position.Z)
				Mouse2.Position = ms
			else
				Mouse1.Position = Vector3.new(100000, 100000, 100000)
				Mouse2.Position = Vector3.new(100000, 100000, 100000)
				Mouse1.Transparency = 10
				Mouse2.Transparency = 10
			end
		end
		if LookPos == true and AimView == true then
			Look1.Transparency = 0
			Look2.Transparency = 0
			Look1.Position = Vector3.new(thrp.Position.X, thrp.Position.Y+0.2, thrp.Position.Z)
			Look2.Position = thrp.Position+look
		else
			Look1.Position = Vector3.new(100000, 100000, 100000) -- Transparency??? bro 
			Look2.Position = Vector3.new(100000, 100000, 100000)
			Look1.Transparency = 10 -- there
			Look2.Transparency = 10
		end
		if AimPov == true then
			possing = true
			cam.CameraSubject = plr.Character.Humanoid
			if PovMode == 1 and HasGun() then
				cam.CFrame = CFrame.new(cam.CFrame.p, ms)
			else
				cam.CFrame = CFrame.new(cam.CFrame.p, cam.CFrame.p+look)
			end
		else
			if possing == true then
				possing = false
				wait(0.025)
				cam.CameraSubject = game:GetService("Players").LocalPlayer.Character.Humanoid
			end
		end
	end
end
game:GetService("RunService").Heartbeat:Connect(function()
	if DesyncTog == false then
		return
	end
	local lp = game:GetService("Players").LocalPlayer
	local hrp = lp.Character.HumanoidRootPart

	--desyncing (beanbot moment??)
	local oldv = hrp.Velocity or Vector3.new(0, 1, 0)
	local newv = Vector3.new(0, 100, 0) -- just in case?
	if DesyncMod == 1 then
		newv = (hrp.CFrame.lookVector * 666) -- spooky devil number :evil:
	else
		newv = Vector3.new(222, 100, -222)
	end
	hrp.Velocity = newv
	hrp.AssemblyLinearVelocity = newv
	game:GetService("RunService").RenderStepped:Wait()
	hrp.Velocity = oldv
	hrp.AssemblyLinearVelocity = oldv -- needed? cba to check
end)
mouse = game.Players.LocalPlayer:GetMouse()
mouse.KeyDown:Connect(function(e)
	if e == key then
		MainHolder.Visible = not MainHolder.Visible
	end
end)
while true do
	task.spawn(function()
		runitback() -- i was gonna do somthing diff at first but couldnt be bothered
	end)
	game:GetService("RunService").Stepped:wait() -- slow it?
end
